{
    "name": "atumsystems/laravel-loki-logging",
    "version": "0.0.1",
    "description": "Logging to Loki for Laravel",
    "type": "library",
    "authors": [
        {
            "name": "AtumG",
            "email": "g@atumsystems.com"
        }
    ],
    "require": {
        "php": "^7.0|^8.0|^8.1",
        "ext-json": "*",
        "illuminate/support": "^7.0.0",
        "illuminate/console": "^7.0.0",
        "monolog/monolog": "^2.0.0"
    },
    "autoload": {
        "psr-4": {
            "AtumSystems\\LaravelLokiLogging\\": "src/"
        }
    },
    "extra": {
        "laravel": {
